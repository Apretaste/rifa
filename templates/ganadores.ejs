<div class="row">
	<div class="col s12">
		<ul class="tabs tabs-fixed-width">
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'RIFA'})">Rifa</a></li>
			<li class="tab"><a href="#" class="active">Ganadores</a></li>
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'RIFA REGLAS'})"><i class="fas fa-question-circle"></i></a></li>
		</ul>
	</div>
</div>

<div class="row">
	<div class="col s12">
		<ul class="collection one-line">
			<% ganadores.forEach(function(item) { %>
				<li class="collection-item avatar">
					<div class="person-avatar circle" face="<%= item.avatar %>" color="<%= item.avatarColor %>" size="40"></div>
					<p><a href="#!" class="black-text <%= item.gender %>" onclick="apretaste.send({command:'PERFIL', data:{username:'<%= item.username %>'}})">@<%= item.username %></a></p>
					<span class="chip clear tiny spacing"><i class="far fa-calendar"></i> <%= moment(item.raffle).format("DD/MM/YYYY") %></span>
					<span class="chip clear tiny"><i class="fas fa-ticket-alt"></i> <%= item.person_tickets %></span>
					<a href="#!" onclick="apretaste.send({command:'PERFIL', data:{username:'<%= item.username %>'}})" class="secondary-content"><i class="material-icons">arrow_forward</i></a>
				</li>
			<% }) %>
		</ul>
	</div>
</div>
